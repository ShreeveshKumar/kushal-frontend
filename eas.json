{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "android": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "ios": {
        "developmentClient": true,
        "distribution": "internal"
      }
    },
    "preview": {
      "android": {
        "distribution": "internal"
      },
      "ios": {
        "distribution": "internal"
      }
    },
    "production": {
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "credentialsSource": "local",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
